{
  "name": "jnhi-plugin",
  "displayName": "极品芝麻官",
  "description": "无限进步！",
  "publisher": "Jnhi",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Other"
  ],
  "repository": "https://github.com/Jnhi/vsCodePlugs.git",
  "icon": "images/icon.png",
  "keywords": [
    "vscode",
    "plugin",
    "demo"
  ],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./dist/main.js",
  "scripts": {
    "updateFont": "node ./build/updateFont.js",
    "showFont": "open ./theme/vscode-10.html",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=dist/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./"
  },
  "contributes": {
    "productIconThemes": [
      {
        "id": "vscode-v1-icons",
        "label": "阿巴阿巴阿巴",
        "path": "./theme/product-icon-theme.json"
      }
    ],
    "configuration": {
			"title": "ClientTool配置",
			"properties": {
				"jnhi-plugin.regExp": {
					"type": "string",
					"default": "G_lang('$1')",
					"description": "多语言匹配格式,$1为匹配符,必须存在"
				}
			}
		},
    "commands": [
      {
        "command": "extension.updataLang",
        "title": "updataLang"
      }
    ]
  },
  
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "esbuild": "^0.13.14",
    "eslint": "^8.1.0",
    "fs": "^0.0.1-security",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "open": "^7.0.3",
    "readline": "^1.3.0",
    "typescript": "^4.4.4",
    "wawoff2": "^2.0.0",
    "webfont": "^11.2.26"
  },
  "dependencies": {
    "cnpm": "^7.1.0",
    "fs": "^0.0.1-security",
    "generator-code": "^1.6.4",
    "packagename": "^2.0.1",
    "path": "^0.12.7",
    "readline": "^1.3.0",
    "tsc": "^2.0.3",
    "yo": "^2.0.2"
  }
}
