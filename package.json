{
  "name": "jnhi-plugin",
  "displayName": "极品芝麻官",
  "description": "无限进步！",
  "publisher": "Jnhi",
  "version": "0.0.1",
  "engines": {
    "vscode": "^0.10.5"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "keywords": ["vscode", "plugin", "demo"],
  "activationEvents": [
    "onLanguage:lua"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "updateFont": "node ./build/updateFont.js",
    "showFont": "open ./theme/vscode-10.html",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "contributes": {
    "productIconThemes": [
      {
        "id": "vscode-v1-icons",
        "label": "VSCode 1.0 Icons",
        "path": "./theme/product-icon-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "open": "^7.0.3",
    "typescript": "^4.4.4",
    "wawoff2": "^2.0.0",
    "webfont": "^11.2.26"
  },
  "dependencies": {
    "cnpm": "^7.1.0",
    "fs": "^0.0.1-security",
    "generator-code": "^1.6.4",
    "packagename": "^2.0.1",
    "path": "^0.12.7",
    "tsc": "^2.0.3",
    "yo": "^2.0.2"
  }
}
